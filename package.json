{
  "name": "@taql",
  "version": "1.0.0",
  "main": "build/index.js",
  "scripts": {
    "format": "prettier --write . || true && eslint --fix . || true",
    "lint": "prettier --write . && eslint --fix --max-warnings 0 --no-error-on-unmatched-pattern .",
    "start": "yarn workspace @taql/server run start",
    "build": "yarn workspaces foreach -tpv run build"
  },
  "engines": {
    "node": "16.18.0",
    "yarn": "3.1.1"
  },
  "workspaces": [
    "packages/*"
  ],
  "repository": "git@gitlab.dev.tripadvisor.com:jacobkatz/taql.git",
  "author": "jacobkatz <jacobkatz@tripadvisor.com>",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  },
  "license": "UNLINCENSED",
  "private": true
}
